{
  "jfetOptions": {
    "solution": "h5act"
  },
  "build": {
    "releasePath": "./h5_act/20180800/test",
    "publicPath": {
      "test": "/h5_act/20180800/test/",
      "prod": "/h5_act/20180800/test/",
      "pack": "../"
    },
    "resolveAlias": {
      "config": "./config"
    },
    "defineConstants": {},
    "sftp": {
      "prod": true
    }
  },
  "server": {
    "port": "3003",
    "opnPath": {
      "local": "/public/pages/index.html",
      "build": "/act/199701/test/pages/index.html"
    },
    "livereload": {
      "watch": "./public/**/*"
    },
    "proxy": [{
      "method": "post",
      "route": "/proxy/base/index",
      "options": {
        "url": "https://swebsit.jyblife.com/base/index",
        "requestOptions": {
          "strictSSL": true
        },
        "host": "https://swebsit.jyblife.com"
      }
    },
    {
      "method": "post",
      "route": "/proxy/act/index",
      "options": {
        "url": "https://swebsit.jyblife.com/act/index",
        "requestOptions": {
          "strictSSL": true
        },
        "host": "https://swebsit.jyblife.com"
      }
    }]
  },
  "image": {
    "input": "../../release/act/199701/test/image/*.{jpg,png,gif}",
    "output": "../../release/act/199701/test/image"
  }
}
