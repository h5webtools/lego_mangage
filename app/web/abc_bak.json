{
  "jfetOptions": {
    "solution": "h5product"
  },
  "build": {
    "releasePath": "../../release/act/199701/test",
    "publicPath": {
      "test": "/act/199701/test/",
      "prod": "/act/199701/test/",
      "pack": "../"
    },
    "defineConstants": {},
    "inPages": true
  },
  "server": {
    "opnPath": {
      "local": "/public/pages/index.html",
      "build": "/act/199701/test/pages/index.html"
    },
    "livereload": {
      "watch": "./public/**/*"
    },
    "proxy": [{
      "method": "post",
      "route": "/proxy/act/index",
