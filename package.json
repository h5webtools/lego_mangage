    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@jyb/eslint-config-jyb": "^1.0.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
